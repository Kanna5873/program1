#include<bits/stdc++.h>
#include<string>
using namespace std;
int main()
{
    string str1,str2;
    cout<<"enter the string";
    getline(cin,str1);
    int n= str1.size();
    int flag[n]={0};
    for(int i=0;i<n;i++)
    {
        int count=0;
        if(flag[i]==0)
        {
            for(int j=0;j<n;j++)
            {
                if(str1[i]==str1[j])
                {
                    count++;
                    flag[j]=1;
                }
            }
            cout<<"occurence of "<<str1[i]<<" "<<count<<endl;
        }
        //cout<<"occurence of "<<str1[i]<<" "<<count<<endl;
    }
    return 0;
}
