#include<iostream>
#include<string>
#include<bits/stdc++.h>
#include<cctype>
using namespace std;
int main()
{
    string str;
    int n;
    cout<<"enter the string";
    getline(cin,str);
    n=str.size();
    for(int i=0;i<n;i++)
    {
        if(isupper(str[i]))
        {
            str[i]=tolower(str[i]);
        }
        else
        str[i]=toupper(str[i]);
    }
    cout<<str;
    return 0;
}
